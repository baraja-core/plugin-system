{
	"name": "baraja-core/plugin-system",
	"description": "Complex plugin system for CMS.",
	"homepage": "https://github.com/baraja-core/plugin-system",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": ">=7.4.0",
		"nette/utils": "^3.0",
		"nette/caching": "^3.0",
		"nette/di": "^3.0",
		"nette/http": "^3.0",
		"tracy/tracy": "^2.7",
		"baraja-core/service-method-invoker": "^1.0"
	},
	"require-dev": {
		"phpstan/phpstan": "^0.12.18",
		"phpstan/phpstan-nette": "^0.12.6",
		"symplify/easy-coding-standard": "^7.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
